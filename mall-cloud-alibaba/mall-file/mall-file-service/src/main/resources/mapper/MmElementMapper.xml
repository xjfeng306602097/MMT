<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.makro.mall.file.mapper.MmElementMapper">

    <resultMap id="BaseResultMap" type="com.makro.mall.file.pojo.entity.MmElement">
